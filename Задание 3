#include <iostream>
using namespace std;
int swap(int& a, int& b) {
	int c = a;
	a = b;
	b = c;
	return 0;
}
int main() {
	setlocale(LC_ALL, "Russian");
	int a, b;
	cout << "Введите первое число:  ";
	cin >> a;
	cout << "Ввдеите второе число:  ";
	cin >> b;
	swap(a,b);
	cout << "Первая переменная:  " << a << endl;
	cout << "Вторая переменная:  " << b;

}
